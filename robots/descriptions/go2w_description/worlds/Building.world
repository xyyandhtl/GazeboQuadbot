        <gravity>0 0 -9.81</gravity>
            <ode>
                <solver>
                    <type>quick</type>  
                    <iters>80</iters>
                    <sor>1.4</sor>
                </solver>  
                <constraints>
                    <cfm>0.0</cfm>
                    <erp>0.15</erp>
                    <!-- <contact_max_correcting_vel>100.0</contact_max_correcting_vel>
                    <contact_surface_layer>0.01</contact_surface_layer> -->
                </constraints>  
            </ode>
        </physics>

