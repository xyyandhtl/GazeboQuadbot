<?xml version="1.0" ?>
<sdf version="1.5">
    <world name="tower">

        <gui fullscreen='false'>
        <camera name='user_camera'>
            <pose>0.5 21 11 0 0.5 -1.6</pose>  <!-- x y z roll pitch yaw -->
            <view_controller>orbit</view_controller>
        </camera>
        </gui>

        <physics type="ode">
        <max_step_size>0.001</max_step_size>
